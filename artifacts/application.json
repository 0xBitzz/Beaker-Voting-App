{
    "hints": {
        "optin_asset": {
            "default_arguments": {
                "aid": {
                    "source": "global-state",
                    "data": "asset_id"
                }
            }
        },
        "transfer_asset": {
            "default_arguments": {
                "aid": {
                    "source": "global-state",
                    "data": "asset_id"
                }
            }
        },
        "get_asset_bal": {
            "default_arguments": {
                "asset_id": {
                    "source": "global-state",
                    "data": "asset_id"
                }
            }
        },
        "get_asset_id": {
            "read_only": true
        },
        "cast_vote": {
            "default_arguments": {
                "asset_id": {
                    "source": "global-state",
                    "data": "asset_id"
                }
            }
        }
    },
    "source": {
        "approval": "",
        "clear": ""
    },
    "schema": {
        "local": {
            "declared": {
                "is_registered": {
                    "type": "uint64",
                    "key": "is_registered",
                    "descr": "Flag to know if an account can vote or not, 1 - True, 0 - False"
                },
                "vote_amount": {
                    "type": "uint64",
                    "key": "vote_amount",
                    "descr": "Amount an account holds at voting time"
                },
                "vote_choice": {
                    "type": "bytes",
                    "key": "vote_choice",
                    "descr": "Choice made by this account, can be either of yes, no, abstain"
                }
            },
            "reserved": {}
        },
        "global": {
            "declared": {
                "asset_id": {
                    "type": "uint64",
                    "key": "asset_id",
                    "descr": "Asset ID"
                },
                "vote_count": {
                    "type": "uint64",
                    "key": "vote_count",
                    "descr": "The accumulated number of votes"
                },
                "reg_begin": {
                    "type": "uint64",
                    "key": "reg_begin",
                    "descr": "Registration window begin time"
                },
                "reg_end": {
                    "type": "uint64",
                    "key": "reg_end",
                    "descr": "Registration window end time"
                },
                "vote_begin": {
                    "type": "uint64",
                    "key": "vote_begin",
                    "descr": "Voting window start time"
                },
                "vote_end": {
                    "type": "uint64",
                    "key": "vote_end",
                    "descr": "Voting window end time"
                }
            },
            "reserved": {}
        }
    },
    "contract": {
        "name": "Vote",
        "methods": [
            {
                "name": "create_asset",
                "args": [
                    {
                        "type": "string",
                        "name": "asset_name"
                    },
                    {
                        "type": "uint64",
                        "name": "total_supply"
                    }
                ],
                "returns": {
                    "type": "void"
                }
            },
            {
                "name": "optin_asset",
                "args": [
                    {
                        "type": "axfer",
                        "name": "txn"
                    },
                    {
                        "type": "asset",
                        "name": "aid"
                    }
                ],
                "returns": {
                    "type": "void"
                }
            },
            {
                "name": "transfer_asset",
                "args": [
                    {
                        "type": "account",
                        "name": "receiver"
                    },
                    {
                        "type": "uint64",
                        "name": "amount"
                    },
                    {
                        "type": "asset",
                        "name": "aid"
                    }
                ],
                "returns": {
                    "type": "void"
                }
            },
            {
                "name": "get_asset_bal",
                "args": [
                    {
                        "type": "account",
                        "name": "account"
                    },
                    {
                        "type": "asset",
                        "name": "asset_id"
                    }
                ],
                "returns": {
                    "type": "uint64"
                }
            },
            {
                "name": "get_asset_id",
                "args": [],
                "returns": {
                    "type": "uint64"
                }
            },
            {
                "name": "setup",
                "args": [
                    {
                        "type": "uint64",
                        "name": "reg_begin"
                    },
                    {
                        "type": "uint64",
                        "name": "reg_end"
                    },
                    {
                        "type": "uint64",
                        "name": "vote_begin"
                    },
                    {
                        "type": "uint64",
                        "name": "vote_end"
                    }
                ],
                "returns": {
                    "type": "void"
                }
            },
            {
                "name": "cast_vote",
                "args": [
                    {
                        "type": "string",
                        "name": "choice"
                    },
                    {
                        "type": "asset",
                        "name": "asset_id"
                    }
                ],
                "returns": {
                    "type": "void"
                }
            },
            {
                "name": "upvote",
                "args": [
                    {
                        "type": "uint64",
                        "name": "amount"
                    }
                ],
                "returns": {
                    "type": "void"
                }
            }
        ],
        "networks": {}
    }
